{
  "name": "@ournet/news-reader",
  "version": "0.0.1",
  "description": "Ournet news-reader app",
  "scripts": {
    "start": "node ./lib/app.js",
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "retest": "ava --no-cache --verbose lib/**/*.test.js",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib",
    "clean-test-code": "rimraf ./lib/**/*.test.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ournet/news-reader.git"
  },
  "keywords": [
    "news-reader",
    "ournet"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ournet/news-reader/issues"
  },
  "homepage": "https://github.com/ournet/news-reader#readme",
  "devDependencies": {
    "@types/entities": "^1.1.0",
    "@types/feedparser": "^2.2.1",
    "@types/got": "^8.3.3",
    "@types/mongodb": "^3.1.4",
    "@types/node": "latest",
    "ava": "^0.23.0",
    "debug": "^3.1.0",
    "rimraf": "^2.6.2",
    "typescript": "latest"
  },
  "dependencies": {
    "ascrape": "^0.2.0",
    "aws-sdk": "^2.296.0",
    "charset": "^1.0.1",
    "color-thief-jimp": "^2.0.2",
    "dotenv": "^6.0.0",
    "ellipsize": "^0.1.0",
    "entities": "^1.1.1",
    "feedparser": "^2.2.9",
    "got": "^9.0.0",
    "iconv-lite": "^0.4.23",
    "imghash": "^0.0.3",
    "in-text-search": "^0.1.0",
    "jimp": "^0.3.11",
    "metascraper": "^3.12.1",
    "mongodb": "^3.1.3",
    "quote-parser": "^0.1.6",
    "rgb-hex": "^2.1.0",
    "sanitize-html": "^1.18.4",
    "striptags": "^3.1.1",
    "winston": "^3.0.0"
  }
}
